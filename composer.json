{
    "name": "ra5k/salud",
    "description": "A simple PHP library",
    "license": "BSD-3-Clause",
    "keywords": ["library", "php", "tools", "framework", "minimal", "oop"],
    "homepage": "https://github.com/ra5k/salud",
    "repositories": [
        
    ],    
    "require": {
        "php": ">=7.0",
        "ext-intl": "*",
        "psr/log": "~1",
        "psr/http-message": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6",
        "symfony/yaml": "~3"
    },
    "suggest": {
        "symfony/yaml": "Enables YAML config file loading"
    },
    "autoload": {
        "psr-4": {
            "Ra5k\\Salud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ra5k\\Salud\\Test\\": "test/src/"
        }
    },
    "scripts": {
        "test": "vendor/phpunit/phpunit/phpunit --bootstrap test/bootstrap.php test/src"
    }    
}
